<h2>Show event</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @event.name %>
  </li>

  <li>
    <strong>Start:</strong>
    <%= @event.start %>
  </li>

  <li>
    <strong>End:</strong>
    <%= @event.end %>
  </li>

</ul>

<%= link "Edit", to: event_path(@conn, :edit, @event) %>
<%= link "Back", to: event_path(@conn, :index) %>
